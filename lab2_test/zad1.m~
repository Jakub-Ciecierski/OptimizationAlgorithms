% --------------------------------------- %
% --------------- INIT ------------------ %
% --------------------------------------- %

clc
clear
format compact

% --------------------------------------- %
% ------------ OPTIMIZATION ------------- %
% --------------------------------------- %

% Set up funtion parameters. Omega
f = [-1; 3; -2; 8; -10];
A = [3, -1, 2, -4, 5;
     -2, 4, 0, -5, 8;
     -4, 3, 8, 20, 2];
b = [7; 12; 7];
lb = [-10;-5;-30;-8;-20];

% Set up options
options = optimset('linprog');
options = optimset(options, ...
                    'LargeScale', 'off', ...
                    'Simplex', 'on', ...
                    'Display', 'iter');
                    
% Compute
[x, f_val, exitFlag, output, lambda] = ...
                %linprog(f, A, b, [], [], lb, [], [], options);

z = f'*x;

x
z
f_val
% exitFlag = 1 if successfull
exitFlag
output
lambda
